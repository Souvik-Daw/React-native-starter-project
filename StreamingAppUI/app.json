      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.fergus2000.StreamingAppUI"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "2f8a9bbe-2b14-4788-9f50-5775d7ad2399"
      }
    },
    "plugins": [
      "expo-router"
    ]
  }
}
